Host github.com
  HostName github.com
  User git
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/github_blog
  IdentitiesOnly yes
Host gcp
  HostName $GCP_IP
  User github_blog
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/github_blog
  IdentitiesOnly yes
Host aliyun
  HostName $ALIYUN_IP:
  User root
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/github_blog
  IdentitiesOnly yes